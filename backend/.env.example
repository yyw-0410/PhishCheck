# Sublime API
SUBLIME_API_KEY=YOUR_SUBLIME_API_KEY
SUBLIME_BASE_URL=https://na-east-3.platform.sublime.security
SUBLIME_TIMEOUT_SECONDS=45
# VirusTotal API
VIRUSTOTAL_API_KEY=YOUR_VIRUSTOTAL_API_KEY
# URLScan API
URLSCAN_API_KEY=YOUR_URLSCAN_API_KEY
# IPQualityScore API
IPQS_API_KEY=YOUR_IPQS_API_KEY
#Hybrid Analysis API
HYBRID_ANALYSIS_API_KEY=YOUR_HYBRID_ANALYSIS_API_KEY

# Google OAuth
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET

# Microsoft OAuth
MS_CLIENT_ID=YOUR_MICROSOFT_CLIENT_ID
MS_CLIENT_SECRET=YOUR_MICROSOFT_CLIENT_SECRET

# AI Service
AI_API_KEY=YOUR_AI_API_KEY
AI_API_KEY_2=YOUR_AI_API_KEY_2
# Set to false to disable AI features (prevents data being sent to Gemini)
AI_ENABLED=true

# Email Verification (Resend)
RESEND_API_KEY=re_your_resend_api_key

# API Protection
# Set to true to require X-API-Key header on analysis endpoints (for ngrok/public demos)
REQUIRE_API_KEY=false

# General Security
SECRET_KEY=YOUR_SECRET_KEY
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Frontend URL - Used for OAuth redirects AND CORS
# Change this when deploying (e.g., https://your-app.ngrok.io)
FRONTEND_URL=http://localhost:5173

# Trusted hosts (FastAPI TrustedHostMiddleware).
BACKEND_TRUSTED_HOSTS=localhost,127.0.0.1

# Runtime mode
ENVIRONMENT=development
DEBUG=True
